<h1>New Game</h1>
<h2><%= @grid.join(" ") %></h2>
<form action="/score" >
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <p>What is the longest form you can find?</p>
  <input type="text" name="guess">
  <%= hidden_field_tag :grid, @grid %>
  <input type="submit" value="Play">
</form>
